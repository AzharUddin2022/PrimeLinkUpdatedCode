<body id="top" class="img js-fullheight theme-particles">
    <!-- preloader
    ================================================== -->
    <div id="preloader">
        <div id="loader"></div>
    </div>
    <div class="s-pagewrap">
        <!-- # header
    ================================================== -->
        <header class="s-header">
            <div class="row s-header__content">

              
            </div> <!-- end s-header__content -->
        </header> <!-- end s-header -->
        <section class="ftco-section">
            <div id="particles-js" class="s-intro__particles"></div>
            <form asp-controller="Login" asp-action="Login" id="frmLogin" class="signin-form">
                
                <div class="container">
                    <div class="row justify-content-center">
                        <div class="col-md-6 text-center mb-5">
                            <h2 class="heading-section">Account Login</h2>
                        </div>
                    </div>
                    <div class="row justify-content-center">
                        <div class="col-md-6 col-lg-4">
                            <div class="login-wrap p-0">
                                <div class="form-group">
                                    <input id="Email" type="text" class="form-control" placeholder="Email" asp-for="Email" required>
                                </div>
                                <div class="form-group">
                                    <input id="Password" type="password" class="form-control" placeholder="Password" asp-for="Password" required>
                                    <span toggle="#Password" class="fa fa-fw fa-eye field-icon toggle-password"></span>
                                </div>
                                <span class="worng-info">@ViewBag.Message</span>
                                <div class="form-group">
                                    <button type="submit" class="form-control btn btn-primary submit px-3">Sign In</button>
                                </div>
                                <span>Don't have an account? </span><a href="#" onclick="SignUpPopup()" style="color: indianred;">Sign up</a>
                            </div>
                        </div>
                    </div>
                </div>
            </form>
        </section>
        <!-- # info
    ================================================== -->
        <section id="info" class="s-info" style="display:none">
        </section> <!-- end s-info -->
    </div>
